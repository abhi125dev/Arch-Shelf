{
  "name": "caratflare-store",
  "version": "0.1.0",
  "license": "ISC",
  "scripts": {
    "dev": "next",
    "predev": "yarn",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "yarn; cross-env NODE_ENV=production && yarn build; rm -rf node_modules; cd functions/; yarn; yarn deploy ; rm -rf node_modules/ ; cd ..",
    "out:deploy": "yarn build && yarn export && yarn deploy",
    "setup": "npm run build-all",
    "next": "npm run build-firebase && npm install",
    "serve": "firebase serve",
    "build-all": "npm run build-next && npm run build-firebase",
    "build-next": "npm install && npm run build",
    "build-firebase": "cd \"functions\" && npm install",
    "lint": "npm run lint:js ",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty . ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "dependencies": {
    "@ant-design/compatible": "^1.0.5",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.8.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "antd": "^4.3.4",
    "axios": "^0.19.2",
    "build": "^0.1.4",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "es6-promise": "4.1.1",
    "export": "^0.1.337",
    "jwt-decode": "^2.2.0",
    "less": "^3.11.2",
    "less-vars-to-js": "^1.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "lodash.range": "^3.2.0",
    "moment": "^2.27.0",
    "multiselect": "^0.9.12",
    "next": "latest",
    "next-compose-plugins": "^2.2.0",
    "next-images": "^1.4.0",
    "next-redux-wrapper": "^6.0.2",
    "qs": "^6.9.4",
    "rc-field-form": "^1.4.4",
    "rc-select": "^10.5.1",
    "react": "^17.0.2",
    "react-alice-carousel": "1.9.5",
    "react-animate-on-scroll": "^2.1.5",
    "react-cookie": "^4.0.3",
    "react-country-flag": "^2.3.0",
    "react-dom": "16.13.1",
    "react-html-parser": "^2.0.2",
    "react-input-mask": "^2.0.4",
    "react-multi-carousel": "^2.5.5",
    "react-password-strength-bar": "^0.3.2",
    "react-phone-number-input": "^3.1.17",
    "react-redux": "^7.2.0",
    "react-reveal": "^1.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-react-hooks": "^4.0.8",
    "next-compose": "^0.0.2"
  }
}
